<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SocialNetwork</display-name>
   <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddImgTypeServlet</servlet-name>
    <servlet-class>com.controller.AddImgTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddImgCatServlet</servlet-name>
    <servlet-class>com.controller.AddImgCatServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PreRegServlet</servlet-name>
    <servlet-class>com.controller.PreRegServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AcceptReqServlet</servlet-name>
    <servlet-class>com.controller.AcceptReqServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SendReqServlet</servlet-name>
    <servlet-class>com.controller.SendReqServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadImageServlet</servlet-name>
    <servlet-class>com.controller.UploadImageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AcceptUserReqServlet</servlet-name>
    <servlet-class>com.controller.AcceptUserReqServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadServlet</servlet-name>
    <servlet-class>com.controller.DownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddImgTypeServlet</servlet-name>
    <url-pattern>/addtype</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddImgCatServlet</servlet-name>
    <url-pattern>/addcat</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PreRegServlet</servlet-name>
    <url-pattern>/prereg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcceptReqServlet</servlet-name>
    <url-pattern>/accept</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SendReqServlet</servlet-name>
    <url-pattern>/sendReq</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadImageServlet</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcceptUserReqServlet</servlet-name>
    <url-pattern>/ac_req</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadServlet</servlet-name>
    <url-pattern>/download</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>